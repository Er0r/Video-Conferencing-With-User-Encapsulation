	<body>
		<nav class="navbar navbar-light bg-light justify-content-between">
			<a class="navbar-brand">Logo</a>
			<div class="">
				<button
					class="py-2 fa bg-light fa-power-off text-danger btn-outline-light fa-lg"
					type="submit"
                    id="logoutbtn"
				></button>
			</div>
		</nav>
		<div class="container">
			<div class="mt-5 col-md-4">
				<div class="p-2" >
					<h2 class="text-center">Current Session</h2>
					<hr />
					<div id="session_contain">
						
					</div>
					
				</div>
			</div>
			<div class="d-flex con_center">
				<div>
					<button
						class="btn btn-default btn-outline-success rounded-pill mr-3 py-2"
						data-toggle="modal"
						data-target="#ModalCenter"
					>
						Schedule Session
					</button>
				</div>
				<div>
					<button
						class="btn btn-default btn-outline-primary rounded-pill ml-3 py-2"
					>
						Instant Meeting
					</button>
				</div>
			</div>
			<div
				class="modal fade"
				id="ModalCenter"
				tabindex="-1"
				role="dialog"
				aria-labelledby=""
				aria-hidden="true"
			>
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<h5 class="offset-md-4" id="ModalLongTitle">Create Session</h5>
							<button
								type="button"
								class="close"
								data-dismiss="modal"
								aria-label="Close"
							>
								<span aria-hidden="true">&times;</span>
							</button>
						</div>
						<div class="modal-body">
							<div class="text-center">
								<span class="form-text small text-danger" id="err-msg"></span>
							</div>
							<div class="">
								<label for="room-name">Session Name: </label>
								<input
									type="text"
									id="room-name"
									class="form-control rounded-0"
									placeholder="Room Name"
								/>
							</div>

							<div class="mb-3">
								<label for="your-name">Session Date: </label>
								<input
									type="datetime-local"
									id="sessiontime"
									class="form-control rounded-0"
									placeholder="Your Name"
								/>
							</div>

							<div class="">
								<button
									id="create-room"
									class="btn btn-block rounded-0 btn-info"
								>
									Create Room
								</button>
							</div>

							<div class="" id="room-created"></div>
						</div>
						<div class="modal-footer">
							<button
								type="button"
								class="btn btn-secondary"
								data-dismiss="modal"
							>
								Close
							</button>
							<button type="button" class="btn btn-primary">
								Save changes
							</button>
						</div>
					</div>
				</div>
			</div>

			<div
				class="modal fade"
				id="ModalCenter_2"
				tabindex="-1"
				role="dialog"
				aria-labelledby=""
				aria-hidden="true"
			>
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="text-center con_center">
						<button class="btn btn-outline-danger modal-content p-2">
							Start Session
						</button>
					</div>
				</div>
			</div>
		</div>

