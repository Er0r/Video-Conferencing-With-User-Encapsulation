		<script>
			function AdminvalidateToken() {
				var flag = 0;
				firebase
					.database()
					.ref("/login/admin")
					.once("value")
					.then((snapshot) => {
						snapshot.forEach((childSnapshot) => {
							if (
								childSnapshot.val().email === sessionStorage.getItem("email") &&
								childSnapshot.val().count > 0
							) {
								flag = 1;
							}
						});
						if (flag === 0) {
							location.replace(`${location.origin}`);
						}
					});
			}
			var token = setInterval(AdminvalidateToken, 2000);
		</script>
